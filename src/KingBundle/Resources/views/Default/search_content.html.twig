<h4 class="page-header center"></h4>
        {% for object in categoryInstances %}
            <div class="col-xs-18 col-sm-6 col-md-3">
                <div class="thumbnail">
                    <a href="#"><img src="{{ asset('logos/'~object.logo) }}" alt=""></a>
                    <div class="caption">
                      <h4>{{ object.name }}</h4>
                      <p>{{ object.description }}</p>
                      <p><a href="#" class="btn btn-info btn-xs" role="button">Find it</a> <a href="#" class="btn btn-default btn-xs" role="button">Learn more</a></p>
                  </div>
                </div>
            </div>
        {% endfor %}
            